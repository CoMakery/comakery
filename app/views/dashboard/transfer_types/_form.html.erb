<%= form_with model: TransferType.new, url: project_dashboard_transfer_types_path(@project), class: 'reg-groups__form', data: { target: 'reg-group-form.form' } do |f| %>
  <%= f.text_field(:name, required: true, placeholder: 'Name') %>
  <%= f.submit('create') %>
  <%= button_tag "cancel", type: 'button', data: {action: 'reg-group-form#hideForm'} %>
<% end %>
